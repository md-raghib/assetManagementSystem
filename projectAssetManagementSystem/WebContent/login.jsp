<body>
	<div>
		
			<img alt="" src="background2.jpg" class="background-image">
		
<div class="row">
		<div id="login" class="col-lg-6">
			
			<form action="#" id="displayform" >
				<div >
				<div  id="loginheader">
					<h1>LOGIN</h1>

				</div>
			</div>
						<div class="form-group">
							<label for="uname" class="control-label">UserName:</label> <input type="text"
